    <taskdef name="instrumentIdeaExtensions" classname="com.intellij.ant.InstrumentIdeaExtensions" classpathref="javac2.classpath"/>
  </target>
  
  <!-- Modules -->
  
  <import file="${basedir}/app/module_app.xml"/>
  
  <import file="${basedir}/module_gpstracker.xml"/>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.app, clean.module.gpstracker" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.app, compile.module.gpstracker" description="build all modules"/>
  
  <target name="all" depends="build.modules" description="build all"/>
</project>